{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*\\+.*\\.js)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*\\+.*\\.css)",
      "destination": "/api/index"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\+.*\\.js|.*\\+.*\\.css)",
      "dest": "/api/index"
    }
  ]
}
        